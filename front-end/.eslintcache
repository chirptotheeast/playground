rt bee from \"../images/smbee.PNG\";\nimport buzz from \"../sounds/buzzverbal.mp3\";\n\nclass Playground extends Component {\n  render() {\n    const audio = new Audio(fairy);\n    const gamesaudio = new Audio(gamesverbal);\n    const drawaudio = new Audio(drawverbal);\n    const zipaudio = new Audio(zipvocal);\n    const snoreaudio = new Audio(snore);\n    const artaudio = new Audio(artverbal);\n    const buzzaudio = new Audio(buzz);\n\n    const start = () => {\n      audio.play();\n    };\n\n    const gamestart = () => {\n      gamesaudio.play();\n    };\n\n    const drawstart = () => {\n      drawaudio.play();\n    };\n\n    const artstart = () => {\n      artaudio.play();\n    };\n\n    const zipstart = () => {\n      zipaudio.play();\n    };\n\n    const snorestart = () => {\n      snoreaudio.play();\n    };\n\n    const buzzstart = () => {\n      buzzaudio.play();\n    };\n    const characterPic = localStorage.getItem(\"character\");\n\n    return (\n      <div>\n        <div className=\"bannerFondo\">\n          <div className=\"float-right \">\n            <button\n              className=\"mt-8 mr-2 btn focus:outline-none\"\n              onClick={this.props.handleLogout}\n            >\n              <Link to=\"/\">logout</Link>\n            </button>\n          </div>\n          <div className=\"float-right\">\n            <Link to=\"/myplayground\">\n              <img\n                className=\"m-4 rounded-full hvr-pulse\"\n                src={characterPic}\n                alt=\"user\"\n              />\n            </Link>\n          </div>\n          <div></div>\n          <div>\n            <div className=\"float-right\">\n              <Link to=\"/\">\n                <img src={bee} className=\"hvr-buzz\" onMouseOver={buzzstart} />\n              </Link>\n            </div>\n          </div>\n        </div>\n        <div className=\"-mt-64 \">\n          <center>\n            <div className=\"w-full text-center\">\n              <h1 className=\
n            <div className=\"bannerFondo \">\n              <div className=\"float-right\">\n                <button className=\"btn\" onClick={this.props.handleLogout}>\n\n                  <Link to=\"/\" >logout</Link>\n                </button>\n              </div>\n              <div className=\"float-right\">\n                <Link to=\"/myplayground\">\n                  <img\n                    className=\"m-4 rounded-full hvr-pulse\"\n                    src={characterPic}\n                    alt=\"user\"\n                  />\n                </Link>\n              </div>\n              <div></div>\n              <div>\n                <div className=\"float-right\">\n                  <Link to=\"/\">\n                    <img\n                      src={bee}\n                      className=\"hvr-buzz\"\n                      onMouseOver={buzzstart}\n                    />\n                  </Link>\n                </div>\n              </div>\n            </div>\n            <div className=\"-mt-64 \">\n              <center>\n                <div className=\"w-full text-center\">\n                  <h1 className=\"text-5xl font-bold text-white\">\n                    <img className=\"playlogo\" src={playlogo} alt=\"logo\" />\n                  </h1>\n                </div>\n              </center>\n            </div>\n\n            <nav className=\"flex items-center min-w-0 px-6 py-4 border-b border-indigo-300 bannerFondo h-1/2\">\n              <div className=\"moveright\"></div>\n              <h1 className=\"text-lg font-semibold\"></h1>\n              <Link to=\"/myplayground\">\n                <img\n                  className=\"w-3/4 playlogo hvr-pulse h-3/4\"\n                  src={myplay}\n                  alt=\"logo\"\n                />\n              </Link>\n              <div className=\"wide-divider\"></div>\n              <Link to=\"/sidewalk\">\n                <img\n                  className=\"hvr-pulse \"\n                  src={sidewalk}\n                  alt=\"logo\"\n                  style={{ float: \"right\" }}\n                />\n              </Link>\n            </nav>\n            <div className=\"smdivider\"></div>\n            <div id=\"wrapper\" className=\"max-w-xl px-4 py-4 mx-auto card-font\">\n              <div className=\"md:grid md:h-32 md:grid-flow-row md:gap-14 md:grid-cols-3\">\n                <div\n                  id=\"jh-stats-positive\"\n                  className=\"flex flex-col justify-center px-8 py-8 bg-white border border-gray-300 rounded shadow-lg hvr-grow-rotate \"\n                >\n                  <Link to=\"/music\">\n                    <div>\n                      <p className=\"text-3xl font-semibold text-center text-gray-800 cursor-pointer p-play \">\n                        music\n                      </p>\n\n                      <img\n                        onMouseOver={start}\n                        className=\"pt-4 playlogo imgnobr hvr-buzz \"\n                        src={musicicon}\n                        alt=\"logo\"\n                      />\n                    </div>\n                  </Link>\n                </div>\n\n                <div\n                  id=\"jh-stats-negative\"\n                  className=\"flex flex-col justify-center px-4 py-4 mt-4 bg-purple-300 border border-gray-300 rounded shadow-lg sm:mt-0 hvr-grow-rotate \"\n                >\n                  <Link to=\"/drawing\">\n                    <div>\n                      <p className=\"text-3xl font-semibold text-center text-gray-800 cursor-pointer p-play\">\n                        draw\n                      </p>\n                      <img\n                        className=\"pt-4 playlogo\"\n                        src={drawicon}\n                        alt=\"logo\"\n                        onMouseOver={drawstart}\n                      />\n                    </div>\n                  </Link>\n                </div>\n\n                <div\n                  id=\"jh-stats-neutral\"\n                  className=\"flex flex-col justify-center px-4 py-4 mt-4 bg-white border border-gray-300 rounded shadow-lg sm:mt-0 hvr-grow-rotate \"\n                >\n                  <Link to=\"/games\">\n                    <div>\n                      <p className=\"text-3xl font-semibold text-center text-gray-800 p-play cursor pointer\">\n                        games\n                      </p>\n                      <img\n                        onMouseOver={gamestart}\n                        className=\"pt-4 playlogo hvr-buzz\"\n                        src={gameicon}\n                        alt=\"logo\"\n                      />\n                    </div>\n                  </Link>\n                </div>\n\n                <div\n                  id=\"jh-stats-positive\"\n                  className=\"flex flex-col justify-center px-8 py-8 bg-purple-300 border-gray-300 rounded shadow-lg hvr-grow-rotate \"\n                >\n                  <Link to=\"/napmusic\">\n                    <div>\n                      <p className=\"text-3xl font-semibold text-center text-gray-800 cursor-pointer p-play \">\n                        nap\n                      </p>\n\n                      <img\n                        onMouseOver={snorestart}\n                        className=\"playlogo imgnobr hvr-buzz pt-9 \"\n                        src={sleepyicon}\n                        alt=\"logo\"\n                      />\n                    </div>\n                  </Link>\n                </div>\n\n                <div\n                  id=\"jh-stats-negative\"\n                  className=\"flex flex-col justify-center px-4 py-4 mt-4 bg-white border border-gray-300 rounded shadow-lg sm:mt-0 hvr-grow-rotate \"\n                >\n                  <Link to=\"/sidewalk\">\n                    <div>\n                      <p className=\"text-3xl font-semibold text-center text-gray-800 cursor-pointer p-play\">\n                        art\n                      </p>\n                      <img\n                        className=\"playlogo\"\n                        src={articon}\n                        alt=\"logo\"\n                        onClick={artstart}\n                      />\n                    </div>\n                  </Link>\n                </div>\n\n                <div\n                  id=\"jh-stats-neutral\"\n                  className=\"flex flex-col justify-center px-4 py-4 mt-4 bg-purple-300 border border-gray-300 rounded shadow-lg sm:mt-0 hvr-grow-rotate \"\n                >\n                  <Link to=\"/puppetshow\">\n                    <div>\n                      <p className=\"text-3xl font-semibold text-center text-gray-800 p-play cursor pointer\">\n                        puppet\n                      </p>\n                      <center>\n                        <img\n                          onClick={zipstart}\n                          className=\"playlogo hvr-buzz \"\n                          src={puppeticon}\n                          alt=\"puppetlogo\"\n                        />\n                      </center>\n                    </div>\n                  </Link>\n                </div>\n              </div>\n            </div>\n            <div className=\"divider\"></div>\n            <div className=\"divider\"></div>\n          </div>\n        );\n                }\n}\n\nexport default Playground","/Users/alyssaeasterly/Playground/front-end/src/reportWebVitals.js",[],["108","109"],"/Users/alyssaeasterly/Playground/front-end/src/index.js",[],"/Users/alyssaeasterly/Playground/front-end/src/components/Games.js",[],["110","111"],"/Users/alyssaeasterly/Playground/front-end/src/components/Music.js",[],"/Users/alyssaeasterly/Playground/front-end/src/components/Drawing.js",[],"/Users/alyssaeasterly/Playground/front-end/src/components/Sidewalk.js",["112","113"],"import React, { Component } from 'react'\nimport sidewalk from \"../images/sidewalk.png\";\nimport CanvasDraw from \"react-canvas-draw\";\nimport { Link } from \"react-router-dom\";\nimport playlogo from \"../images/sunnyplay.png\";\nimport myplay from \"../images/bigmyplayground.png\";\nimport bee from '../images/bee.PNG'\n\n// import Like from \"../components/Like.js\"\n\nexport default class Thesidewalk extends Component {\n  \n  likeHandler = (drawingid) => {\n    //console.log(drawingsvgdrawing)\n    const currentUser = localStorage.getItem(\"userId\");\n    const userId = parseInt(currentUser, 10);\n   const drawCode = localStorage.getItem('favDrawingStr')\n\n   \n    \n\n\n    let data = {\n      user_id: userId,\n      drawing_id: drawingid,\n      drawing: drawCode\n    };\n    \n\n    const requestOptions = {\n      method: \"POST\",\n      headers: { \"Content-Type\": \"application/json\" },\n      body: JSON.stringify({ data }),\n    };\n\n    fetch(\"http://localhost:3000/favorite_drawings\", requestOptions)\n      .then((response) => response.json())\n      .then((data) => console.log(data));\n  };\n\n  render() {\n \n   \n    const characterPic = localStorage.getItem(\"character\");\n\n    return (\n      <div>\n        <div className=\"bannerFondo\">\n          <div className=\"float-right\">\n            <Link to=\"/myplayground\">\n              <img\n                className=\"rounded-full m-4 hvr-pulse\"\n                src={characterPic}\n                alt=\"user\"\n              />\n            </Link>\n          </div>\n        </div>\n\n        <div className=\"-mt-64 \">\n          <div className=\"w-full text-center\">\n            <h1 className=\"font-bold text-5xl text-white\">\n              <img className=\"playlogo\" src={sidewalk} alt=\"logo\" />\n            </h1>\n          </div>\n        </div>\n\n        <nav className=\"border-b border-indigo-300 bg-white px-7 py-4 flex items-center min-w-0 h-17\">\n          <h1 className=\"font-semibold text-lg\"></h1>\n\n          <Link to=\"/playground\">\n            <img className=\"hvr-pulse\" src={playlogo} alt=\"logo\" />\n          </Link>\n          <div className=\"wide-divider\"></div>\n          <Link to=\"/myplayground\">\n            <img className=\" hvr-pulse\" src={myplay} alt=\"myplay\" />\n          </Link>\n        </nav>\n        <center>\n          <div className=\"container items-center pt-20\">\n            <div className=\"bg-white bg-opacity-95 w-1/2 flex items-center p-2 rounded-xl shadow border-double border-8 border-yellow-300\">\n              <div className=\"flex items-center \"></div>\n              <div className=\"flex-grow p-2\">\n                <div className=\"pageheading\">\n                  click on drawing to add to favorites\n                  <img\n                    src={bee}\n                    alt=\"My profile\"\n                    className=\"w-16 h-16  hvr-buzz\"\n                  />\n                </div>\n              </div>\n            </div>\n          </div>\n        </center>\n\n        <div className=\"grid grid-cols-3 gap-4 place-content-center -m-4\">\n          {this.props.drawings.map((drawing) => {\n            return (\n              <div className=\"flex-container \">\n                {/* {console.log(drawing.id)} */}\n                <div\n                  className=\"box-border lg:box-content h-480 w-480 p-6 border-4 bg-green-100 bg-opacity-50 shadow \"\n                  onClick={() => {\n                    // const findId = drawingIdArr.find((drawId) => {\n                    //   return drawId === drawing.Id;\n                    // });\n                    // console.log(findId)\n                    localStorage.setItem(\"favDrawingStr\", drawing.svgdrawing);\n                    {\n                      this.likeHandler(drawing.id);\n                    }\n                  }}\n                >\n                  <CanvasDraw\n                    className=\"shadow\"\n                    disabled\n                    hideGrid\n                    ref={